{
  "name": "Youtube - Skip Ad",
  "description": "This extension skip youtube ad automaticlly",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "128": "YouExLogo.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting",
    "notifications"
  ],
  "host_permissions": [
    "https://*/*"
],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}